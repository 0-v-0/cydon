    "autoprefixer": "^10.4.7",
    "eslint": "^8.18.0",
    "postcss": "^8.4.14",
    "stylelint": "^14.9.1",
    "stylelint-stylus": "^0.16.1",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.4",
    "unocss": "^0.38.2",
    "vite": "^2.9.12",
    "vite-plugin-inspect": "^0.5.0",
    "vite-plugin-progress": "^0.0.2"
  },
  "dependencies": {
    "@cydon/ustyle": "workspace:*",
